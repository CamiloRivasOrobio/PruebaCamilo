@model sistema.Models.InfoConsultaViewModel

@await Html.PartialAsync("_MensajeExito")

<div class="main-card mb-3 card">
    <div class="card-body">
        <div class="text-center">
            <h4><b>Consulta No. @Model.Consulta.Id</b></h4>
        </div>
    </div>
</div>

<div class="main-card mb-3 card">
    <div class="card-header">
        <div class="btn-actions-pane-right">
            <div class="nav">
                <a data-toggle="tab" href="#tab1-eg2-0"
                   class="btn-pill btn-wide mr-1 ml-1 active btn btn-outline-alternate btn-sm">Datos del paciente</a>
                <a data-toggle="tab" href="#tab1-eg2-1"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">General</a>
                <a data-toggle="tab" href="#tab1-eg2-2"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Dental</a>
                <a data-toggle="tab" href="#tab1-eg2-3"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Médico</a>
                <a data-toggle="tab" href="#tab1-eg2-4"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Pediátricos</a>
                <a data-toggle="tab" href="#tab1-eg2-5"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Ginecológicos</a>
                <a data-toggle="tab" href="#tab1-eg2-6"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Neurológicos</a>
                <a data-toggle="tab" href="#tab1-eg2-7"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Patológico</a>
                <a data-toggle="tab" href="#tab1-eg2-8"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Nutricional</a>
                <a data-toggle="tab" href="#tab1-eg2-9"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Estético</a>
                <a data-toggle="tab" href="#tab1-eg2-10"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Rev. por sistemas</a>
                <a data-toggle="tab" href="#tab1-eg2-11"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Área terapéutica</a>
                <a data-toggle="tab" href="#tab1-eg2-12"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Prescripción</a>
            </div>
        </div>
    </div>
    <div class="card-body">
        <div class="tab-content">
            <div class="tab-pane active" id="tab1-eg2-0" role="tabpanel">
                <div class="form-row">
                    <div class="col-md-12">
                        <h5><b>Datos personales</b></h5>
                        <hr>
                    </div>
                    <div class="col-md-12">
                        <b>Nombre: </b>@Model.Consulta.Citas.Paciente.Nombre<br />
                        <b>NIT: </b>@Model.Consulta.Citas.Paciente.Nit<br />
                        <b>Dirección: </b>@Model.Consulta.Citas.Paciente.Direccion<br />
                        <b>Alias: </b>@Model.Consulta.Citas.Paciente.Alias<br />
                        <b>Sexo: </b>@Model.Consulta.Citas.Paciente.Sexo.DescripcionSexo<br />
                        <b>Fecha de nacimiento: </b>@Model.Consulta.Citas.Paciente.FechaNacimiento<br />
                        <b>Edad: </b>@Model.Consulta.Citas.Paciente.Edad<br />
                        <b>Teléfono: </b>@Model.Consulta.Citas.Paciente.Telefono<br />
                        <b>Celular: </b>@Model.Consulta.Citas.Paciente.Celular
                    </div>
                </div>
                <br /><br />
                <div class="form-row">
                    <div class="col-md-12">
                        <h5><b>Antecedentes</b></h5>
                        <hr />
                    </div>
                    <div class="col-md-12">
                        <b>Antecedentes médicos: </b>@Model.Consulta.Citas.Paciente.AntecedentesMedicos<br />
                        <b>Antecedentes quirúrgicos: </b>@Model.Consulta.Citas.Paciente.AntecedentesQuirurgicos <br />
                        <b>Antecedentes traumáticos: </b>@Model.Consulta.Citas.Paciente.AntecedentesTraumaticos<br />
                        <b>Alergias: </b>@Model.Consulta.Citas.Paciente.AntecedentesAlergias<br />
                        <b>Vicios: </b>@Model.Consulta.Citas.Paciente.AntecedentesVicios<br />
                        <b>Medicamentos: </b>@Model.Consulta.Citas.Paciente.AntecedentesMedicamentos
                    </div>
                </div>
                <br /><br />
                <div class="form-row">
                    <div class="col-md-12">
                        <a asp-controller="Pacientes" asp-action="Modificar" asp-route-id="@Model.Consulta.Citas.PacienteId"
                           class="btn btn-primary text-white">
                            Editar datos del paciente&nbsp;&nbsp;&nbsp;&nbsp;<i class="fas fa-edit"></i>
                        </a>
                    </div>
                </div>
            </div>
            <div class="tab-pane" id="tab1-eg2-1" role="tabpanel">
                <div class="form-row">
                    <div class="col-md-12">
                        <h5><b>Información de consulta</b></h5>
                        <hr>
                    </div>
                    <div class="col-md-3">
                        <div class="position-relative form-group">
                            <label for="exampleEmail11" class="">Especialidad</label>
                            @Html.TextBoxFor(a => a.Consulta.Citas.EspecialidadText, new
                            {
                            @class = "form-control",
                            @readonly = true
                            })
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="position-relative form-group">
                            <label for="exampleEmail11" class="">Servicio</label>
                            @Html.TextBoxFor(a => a.Consulta.Citas.ServicioText, new
                            {
                            @class = "form-control",
                            @readonly = true
                            })
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="position-relative form-group">
                            <label for="exampleEmail11" class="">Médico</label>
                            @Html.TextBoxFor(a => a.Consulta.Citas.EmpleadoText, new
                            {
                            @class = "form-control",
                            @readonly = true
                            })
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="position-relative form-group">
                            <label for="exampleEmail11" class="">Motivo</label>
                            @Html.TextAreaFor(a => a.Consulta.Citas.Motivo, new
                            {
                            @class = "form-control",
                            @readonly = true
                            })
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="position-relative form-group">
                            <label for="exampleEmail11" class="">Costo de consulta</label>
                            @Html.TextBoxFor(a => a.Consulta.CostoConsulta, new
                            {
                            @class = "form-control",
                            @readonly = true
                            })
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="position-relative form-group">
                            <label>Estado de pago</label>
                            @Html.TextBoxFor(m => m.Consulta.EstadoPagoConsulta.Estado,
                            new { @class = "form-control", @readonly = true })
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="position-relative form-group">
                            <label>Fecha de creación de consulta</label>
                            <input class="form-control" asp-for="Consulta.FechaYHoraInicioConsulta" readonly="readonly">
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="position-relative form-group">
                            <label>Fecha de próxima consulta</label>
                            <input class="form-control" asp-for="Consulta.FechaProximaConsulta" readonly>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="position-relative form-group">
                            <label>Fecha de su última radiografía/TAC/Resonancia magnética</label>
                            @Html.TextBoxFor(a => a.Consulta.FechaUltimaRadiografiaDental,
                            new { @class = "form-control", @readonly = true })
                        </div>
                    </div>

                    <div class="col-md-3">
                        <label>Tipo de consulta</label>
                        @Html.TextBoxFor(m => m.Consulta.TipoConsulta, new { @class = "form-control", @readonly = true })
                    </div>

                    <div class="col-md-3">
                        <label>Tipo de referencia</label>
                        @Html.TextBoxFor(m => m.Consulta.TipoReferencia, new { @class = "form-control", @readonly = true })
                    </div>

                    <div class="col-md-3">
                        <label>Medico Referido</label>
                        @Html.TextBoxFor(m => m.Consulta.MedicoReferido, new { @class = "form-control", @readonly = true })
                    </div>

                    <div class="col-md-12">
                        <label>Anotaciones u Observaciones</label>
                        @Html.TextAreaFor(m => m.Consulta.ObservacionesAdicionales, new { @class = "form-control", @readonly = true })
                    </div>
                </div>
                <br /><br />
                <div class="form-row">
                    <div class="col-md-12">
                        <h5><b>Servicios</b></h5>
                        <hr>
                    </div>
                    <div class="col-md-12">
                        <br>
                        <div class="table-responsive">
                            <table class='table table-hover' id="tabla-servicios">
                                <thead>
                                    <tr>
                                        <th>Id</th>
                                        <th>Diente</th>
                                        <th>Nombre servicio</th>
                                        <th>Precio</th>
                                    </tr>
                                </thead>
                                <tbody id="contenido-servicios">
                                    @foreach (var servicio in Model.Consulta.ConsultasServicios)
                                    {
                                        <tr>
                                            <td>@servicio.Servicio.Id</td>
                                            <td>@servicio.NumeroDiente</td>
                                            <td>@servicio.Servicio.NombreServicio</td>
                                            <td>@servicio.Servicio.Precio</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <br /><br />
                <div class="form-row">
                    <div class="col-md-12">
                        <h5><b>Solo para mujeres</b></h5>
                        <hr>
                    </div>
                    <div class="col-md-4">
                        ¿Está usted embarazada?<br>
                        @switch (Model.Consulta.EstaEmbarazada)
                        {
                            case "si":
                                <span><b>Sí</b></span>
                                break;
                            case "no":
                                <span><b>No</b></span>
                                break;
                            case "ns":
                                <span><b>N/S</b></span>
                                break;
                            default:
                                <span><b>-</b></span>
                                break;
                        }
                        <br>
                        Número de semanas<br>
                        @Html.TextBoxFor(a => a.Consulta.NumeroSemanasEmbarazo, new
                        {
                        @class = "form-control",
                        @readonly = true
                        })
                    </div>
                    <div class="col-md-4">
                        ¿Está usted tomando píldoras anticonceptivas o de sustitución hormonal?<br>
                        @switch (Model.Consulta.TomaPildorasAnticonceptivas)
                        {
                            case "si":
                                <span><b>Sí</b></span>
                                break;
                            case "no":
                                <span><b>No</b></span>
                                break;
                            case "ns":
                                <span><b>N/S</b></span>
                                break;
                            default:
                                <span><b>-</b></span>
                                break;
                        }
                    </div>
                    <div class="col-md-4">
                        ¿Está usted amamantando?<br>
                        @switch (Model.Consulta.EstaAmamantando)
                        {
                            case "si":
                                <span><b>Sí</b></span>
                                break;
                            case "no":
                                <span><b>No</b></span>
                                break;
                            case "ns":
                                <span><b>N/S</b></span>
                                break;
                            default:
                                <span><b>-</b></span>
                                break;
                        }
                    </div>
                </div>
                <br /><br />
                <div class="form-row">
                    <div class="col-md-12">
                        <h5><b>Bebidas alcohólicas</b></h5>
                        <hr>
                    </div>
                    <div class="col-md-12">
                        <div class="form-row">
                            <div class="col-md-4">
                                ¿Bebe bebidas alcohólicas?<br />
                                @switch (Model.Consulta.BebeBebidasAlcoholicas)
                                {
                                    case "si":
                                        <span><b>Sí</b></span>
                                        break;
                                    case "no":
                                        <span><b>No</b></span>
                                        break;
                                    case "ns":
                                        <span><b>N/S</b></span>
                                        break;
                                    default:
                                        <span><b>-</b></span>
                                        break;
                                }
                            </div>
                            <div class="col-md-4">
                                Si es así, ¿cuánto alcohol bebió en las últimas 24 horas?<br>
                                @Html.TextBoxFor(a => a.Consulta.AlcoholUltimas24Horas,
                                new { @class = "form-control", @readonly = true })
                            </div>
                            <div class="col-md-4">
                                Si es así, ¿cuánto bebe por lo general en una semana?<br>
                                @Html.TextBoxFor(a => a.Consulta.AlcoholSemanal,
                                new { @class = "form-control", @readonly = true })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
