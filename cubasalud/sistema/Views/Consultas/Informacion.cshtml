                    @foreach (var item in Model.Consulta.ConsultasCaracteristicasDentales)
                    {
                        <div class="form-row col-md-12" id="servicios-diente-@item.NumeroDiente" style="display:none">
                            <div class="col-md-12">
                                <h5><b>Servicios diente No. @item.NumeroDiente</b></h5>
                                <hr>
                            </div>
                            <div class="col-md-12">
                                <div class="table-responsive">
                                    <table class='table table-hover'>
                                        <thead>
                                            <tr>
                                                <th>Id</th>
                                                <th>Nombre servicio</th>
                                                <th>Precio</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var servicio in Model.Consulta.ConsultasServicios)
                                            {
                                                @if (servicio.NumeroDiente == item.NumeroDiente)
                                                {
                                                    <tr>
                                                        <td>@servicio.Servicio.Id</td>
                                                        <td>@servicio.Servicio.NombreServicio</td>
                                                        <td>@servicio.Servicio.Precio</td>
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>
                                </div>
                                <br /><br />
                            </div>
                        </div>
                        <div class="col-md-12 p-0 form-row" id="caracteristicas-diente-@item.NumeroDiente" style="display:none">
                            <div class="col-md-2" style="padding:5px">
                                <div class="col-md-12 p-0" style="box-shadow:1px 1px 5px 0px lightgray; gray;border-radius:10px">
                                    <div class="col-md-12 p-2" style="background-color: lightgray;border-top-left-radius:10px;border-top-right-radius:10px">
                                        <center><b>PERCUSIONES</b></center>
                                    </div>
                                    <div class="p-2">
                                        Diente No. <span><b>@item.NumeroDiente</b></span><br>
                                        @if (item.Percusiones_VerticalMas)
                                        {
                                            <span><b>Vertical +</b></span>
                                            <br />
                                        }
