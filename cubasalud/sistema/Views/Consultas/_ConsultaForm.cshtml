@model sistema.Models.ConsultasViewModel
@Html.HiddenFor(a => a.CitaId)
@Html.HiddenFor(a => a.ConsultaId)
@Html.HiddenFor(a => a.PrimeraConsulta)
@Html.HiddenFor(a => a.HistoriaId)
@Html.HiddenFor(a => a.ExamenFisicoId)

<div class="main-card mb-3 card">
    <div class="card-body">
        <div id="tabs">
            <ul>
                <li><a href="#tab-datos-paciente">Datos del paciente</a></li>
                <li><a href="#tab-general">General</a></li>
                <li><a href="#tab-dental">Dental</a></li>
                <li><a href="#tab-medico">Médico</a></li>
                <li><a href="#tab-pediatricos">Pediátricos</a></li>
                <li><a href="#tab-ginecologicos">Ginecológicos</a></li>
                <li><a href="#tab-neurologicos">Neurológicos</a></li>
                <li><a href="#tab-nutricional">Nutricional</a></li>
                <li><a href="#tab-examenes">Exámenes de laboratorio</a></li>
                <li><a href="#tab-estetico">Estético</a></li>
                <li><a href="#tab-rev-sistemas">Rev. por sistemas</a></li>
                <li><a href="#tab-terapeutico">Área terapéutica</a></li>
            </ul>
            <div id="tab-datos-paciente">
                <div class="form-row">
                    <div class="col-md-12">
                        <h5><b>Datos personales</b></h5>
                        <hr>
                    </div>
                    <div class="col-md-12">
                        <b>Nombre: </b>@Model.PacienteNombre<br />
                        <b>NIT: </b>@Model.PacienteNit<br />
                        <b>Dirección: </b>@Model.PacienteDireccion<br />
                        <b>Alias: </b>@Model.PacienteAlias<br />
                        <b>Sexo: </b>@Model.PacienteSexo<br />
                        <b>Fecha de nacimiento: </b>@Model.PacienteFechaNacimiento<br />
                        <b>Edad: </b>@Model.PacienteEdad<br />
                        <b>Teléfono: </b>@Model.PacienteTelefono<br />
                        <b>Celular: </b>@Model.PacienteCelular
                    </div>
                </div>
                <br /><br />
                <div class="form-row">
                    <div class="col-md-12">
                        <h5><b>Antecedentes</b></h5>
                        <hr />
                    </div>
                    <div class="col-md-12">
                        <b>Antecedentes médicos: </b>@Model.PacienteMedicos<br />
                        <b>Antecedentes quirúrgicos: </b>@Model.PacienteQuirurgicos <br />
                        <b>Antecedentes traumáticos: </b>@Model.PacienteTraumaticos<br />
                        <b>Alergias: </b>@Model.PacienteAlergias<br />
                        <b>Vicios: </b>@Model.PacienteVicios<br />
                        <b>Medicamentos: </b>@Model.PacienteMedicamentos
                    </div>
                </div>
                <br /><br />
                <div class="form-row">
                    <div class="col-md-12">
                        <a asp-controller="Pacientes" asp-action="Modificar" asp-route-id="@Model.PacienteId"
                           class="btn btn-primary text-white">
                            Editar datos del paciente&nbsp;&nbsp;&nbsp;&nbsp;<i class="fas fa-edit"></i>
                        </a>
                    </div>
                </div>
            </div>
            <div id="tab-general">
                <div class="form-row">
                    <div class="col-md-12">
                        <h5><b>Información de consulta</b></h5>
                        <hr>
                    </div>
                    <div class="col-md-3">
                        <div class="position-relative form-group">
                            <label for="exampleEmail11" class="">Costo de consulta</label>
                            @Html.TextBoxFor(a => a.CostoConsulta, new
                            {
                            @class = "form-control",
                            @placeholder = "Costo de consulta"
                            })
                            @Html.ValidationMessageFor(a => a.CostoConsulta, "", new { @class = "text-danger" })
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="position-relative form-group">
                            <label>Estado de pago</label>
                            <select class="form-control select2bs4" style="width: 100%;" asp-for="EstadoPagoId"
                                    asp-items="@Model.EstadosPagosLista">
                            </select>
                        </div>
                    </div>
                    @* <div class="col-md-3">
                        <div class="position-relative form-group">
                        <label>Cuentas por cobrar</label>
                        @Html.TextBoxFor(m => m.CuentasPorCobrar, new { @class = "form-control" })
                        @Html.ValidationMessageFor(a => a.CuentasPorCobrar, "", new { @class = "text-danger" })
                        </div>
                        </div> *@
                    <div class="col-md-3">
                        <div class="position-relative form-group">
                            <label>Fecha de creación de consulta</label>
                            <input class="form-control" asp-for="FechaYHoraInicio" readonly="readonly">
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="position-relative form-group">
                            <label>Fecha de próxima consulta</label>
                            <input class="form-control" asp-for="FechaProximaConsulta">
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="position-relative form-group">
                            <label>Fecha de su último examen</label>
                            <input class="form-control" asp-for="FechaUltimaConsulta" readonly="readonly">
                        </div>
                    </div>

                    <div class="col-md-12">
                        <div class="position-relative form-group">
                            <label>¿Qué le hicieron en esa ocasión?</label>
                            <input class="form-control" asp-for="MotivoUltimaConsulta" readonly="readonly">
                        </div>
                    </div>

                    <div class="col-md-3">
                        <div class="position-relative form-group">
                            <label>Fecha de su última radiografía/TAC/Resonancia magnética</label>
                            @Html.TextBoxFor(a => a.FechaUltimaRadiografiaDental, new { @type = "date", @class = "form-control" })
                        </div>
                    </div>

                    <div class="col-md-3">
                        <label>Tipo de consulta</label>
                        <select class="form-control select2bs4" style="width: 100%;" asp-for="TipoConsulta">
                            <option disabled selected>--- SELECCIONE ---</option>
                            <option value="Primera Consulta">Primera Consulta</option>
                            <option value="Nueva Consulta">Nueva Consulta</option>
                            <option value="Reconsulta">Reconsulta</option>
                            <option value="Observacion">Observacion</option>
                        </select>
                    </div>

                    <div class="col-md-3">
                        <label>Tipo de referencia</label>
                        <select class="form-control select2bs4" style="width: 100%;" asp-for="TipoReferencia">
                            <option disabled selected>--- SELECCIONE ---</option>
                            <option value="Hospital Regional">Hospital Regional</option>
                            <option value="Medico Particular">Medico Particular</option>
                        </select>
                    </div>

                    <div class="col-md-3">
                        <label>Medico Referido</label>
                        <input class="form-control" asp-for="MedicoReferido">
                        @Html.ValidationMessageFor(a => a.MedicoReferido, "", new { @class = "text-danger" })
                    </div>

                    <div class="col-md-12">
                        <label>Anotaciones u Observaciones</label>
                        <textarea class="form-control" asp-for="ObservacionesAdicionales"
                                  placeholder="Anotaciones u Observaciones"></textarea>
                        @Html.ValidationMessageFor(a => a.ObservacionesAdicionales, "", new { @class = "text-danger" })
                    </div>
                </div>
                <br /><br />
                <div class="form-row">
                    <div class="col-md-12">
                        <h5><b>Servicios</b></h5>
                        <hr>
                    </div>
                    <div class="col-md-7">
                        <select class="form-control" data-bind="options: servicios,
                                                                                optionsText: 'nombreServicio',
                                                                                value: servicioSeleccionado,
                                                                                event: {change:consultarPrecios}">
                        </select>
                    </div>
                    <div class="col-md-3">
                        <select class="form-control" data-bind="options: precios,
                                                                                optionsText: 'Precio',
                                                                                value: precioSeleccionado"></select>
                    </div>
                    <div class="col-md-2">
                        <a class="btn btn-primary" style="color:white"
                           data-bind="click: agregarServicioConsulta">
                            <i class="fas fa-add"></i> Agregar
                        </a>
                    </div>
                    <div class="col-md-12">
                        <br>
                        <div class="table-responsive">
                            <table class='table table-hover'>
                                <thead>
                                    <tr>
                                        <th>Id</th>
                                        <th>Nombre servicio</th>
                                        <th>Precio</th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <!--ko foreach: serviciosAgregadosConsulta-->
                                    <tr>
                                        <td data-bind="text: ServicioId"></td>
                                        <td data-bind="text: NombreServicio"></td>
                                        <td data-bind="text: Precio"></td>
                                        <td>
                                            <a class="btn btn-primary"
                                               style="color:white" data-bind="click: $root.quitarServicioConsulta">Quitar</a>
                                        </td>
                                    </tr>
                                    <!--/ko-->
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <br /><br />
                <div class="form-row">
                    <div class="col-md-12">
                        <h5><b>Solo para mujeres</b></h5>
                        <hr>
                    </div>
                    <div class="col-md-4">
                        ¿Está usted embarazada?<br>
                        @Html.RadioButtonFor(m => m.EstaEmbarazada, "si") Sí<br>
                        Número de semanas<br>
                        @Html.TextBoxFor(a => a.NumeroSemanasEmbarazo, new
                        {
                        type = "number",
                        @class = "form-control",
                        @placeholder = "Número de semanas"
                        })
                        @Html.RadioButtonFor(m => m.EstaEmbarazada, "no") No<br>
                        @Html.RadioButtonFor(m => m.EstaEmbarazada, "ns") N/S
                    </div>
                    <div class="col-md-4">
                        ¿Está usted tomando píldoras anticonceptivas o de sustitución hormonal?<br>
                        @Html.RadioButtonFor(m => m.TomaPildorasAnticonceptivas, "si") Sí<br>
                        @Html.RadioButtonFor(m => m.TomaPildorasAnticonceptivas, "no") No<br>
                        @Html.RadioButtonFor(m => m.TomaPildorasAnticonceptivas, "ns") N/S
                    </div>
                    <div class="col-md-4">
                        ¿Está usted amamantando?<br>
                        @Html.RadioButtonFor(m => m.EstaAmamantando, "si") Sí<br>
                        @Html.RadioButtonFor(m => m.EstaAmamantando, "no") No<br>
                        @Html.RadioButtonFor(m => m.EstaAmamantando, "ns") N/S
                    </div>
                </div>
                <br /><br />
                <div class="form-row">
                    <div class="col-md-12">
                        <h5><b>Bebidas alcohólicas</b></h5>
                        <hr>
                    </div>
                    <div class="col-md-12">
                        <div class="form-row">
                            <div class="col-md-12">
                                ¿Bebe bebidas alcohólicas?
                            </div>
                            <div class="col-md-6">
                                <div class="col-md-2">
                                    @Html.RadioButtonFor(m => m.BebeBebidasAlcoholicas, "si") Sí
                                </div>
                                <div class="col-md-4">
                                    Si es así, ¿cuánto alcohol bebió en las últimas 24 horas?<br>
                                    @Html.TextBoxFor(a => a.AlcoholUltimas24Horas, new { @class = "form-control" })
                                </div>
                                <div class="col-md-4">
                                    Si es así, ¿cuánto bebe por lo general en una semana?<br>
                                    @Html.TextBoxFor(a => a.AlcoholSemanal, new { @class = "form-control" })
                                </div>
                            </div>
                            <div class="col-md-3">
                                @Html.RadioButtonFor(m => m.BebeBebidasAlcoholicas, "no") No
                            </div>
                            <div class="col-md-3">
                                @Html.RadioButtonFor(m => m.BebeBebidasAlcoholicas, "ns") N/S
                            </div>
                        </div>
                    </div>
                </div>
            </div>
