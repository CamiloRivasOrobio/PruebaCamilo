@model sistema.Models.PacientesBaseViewModel
@Html.HiddenFor(a => a.PacienteId)

<div class="main-card mb-3 card">
    <div class="card-header">
        <div class="btn-actions-pane-right">
            <div class="nav">
                <a data-toggle="tab" href="#tab-eg2-0"
                   class="btn-pill btn-wide mr-1 ml-1 active btn btn-outline-alternate btn-sm">Datos personales</a>
                <a data-toggle="tab" href="#tab-eg2-1"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Antecedentes</a>
                <a data-toggle="tab" href="#tab-eg2-2"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Médico</a>
                <a data-toggle="tab" href="#tab-eg2-3"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Historia médica</a>
                <a data-toggle="tab" href="#tab-eg2-4"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Pediátricos</a>
                <a data-toggle="tab" href="#tab-eg2-5"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Ginecológicos</a>
                <a data-toggle="tab" href="#tab-eg2-6"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Neurológicos</a>
                <a data-toggle="tab" href="#tab-eg2-7"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Políticas de pago</a>
                <a data-toggle="tab" href="#tab-eg2-8"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Declaración de consentimiento</a>
                <a data-toggle="tab" href="#tab-eg2-9"
                   class="btn-pill btn-wide mr-1 ml-1 btn btn-outline-alternate btn-sm">Datos de pago</a>
            </div>
        </div>
    </div>
    <div class="card-body">
        <div class="tab-content">
            <div class="tab-pane active" id="tab-eg2-0" role="tabpanel">
                <div class="main-card mb-3 card col-md-12">
                    <div class="card-body">
                        <div class="form-row">
                            <div class="col-md-12">
                                <h5><b>Datos personales</b></h5>
                                <hr>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Fecha de registro</label>
                                    @Html.TextBoxFor(a => a.FechaRegistro, "{0:yyyy-MM-dd}", new
                                    {
                                    @class = "form-control",
                                    @type = "date"
                                    })
                                    @Html.ValidationMessageFor(a => a.FechaRegistro, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Nombre completo del paciente</label>
                                    @Html.TextBoxFor(a => a.Nombre, new { @class = "form-control", @placeholder = "Nombre Paciente" })
                                    @Html.ValidationMessageFor(a => a.Nombre, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Alias del paciente</label>
                                    @Html.TextBoxFor(a => a.Alias, new { @class = "form-control", @placeholder = "Alias del paciente" })
                                    @Html.ValidationMessageFor(a => a.Alias, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Sexo</label>
                                    <select class="form-control select2bs4" style="width: 100%;" asp-for="@Model.SexoId"
                                            asp-items="@Model.SexoSelectListItems">
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Fecha de nacimiento</label>
                                    @Html.TextBoxFor(a => a.FechaNacimiento, "{0:yyyy-MM-dd}", new
                                    {
                                    @class = "form-control",
                                    @type = "date",
                                    @onchange = "getEdad()",
                                    @onkeyup = "getEdad()"
                                    })
                                    @Html.ValidationMessageFor(a => a.FechaNacimiento, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Edad</label>
                                    @Html.TextBoxFor(a => a.Edad, new { @class = "form-control", @type = "text", @readonly = "true" })
                                    @Html.ValidationMessageFor(a => a.Edad, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Tipo de sangre</label>
                                    @Html.TextBoxFor(a => a.TipoDeSangre, new
                                    {
                                    @class = "form-control",
                                    @placeholder = "Tipo de sangre"
                                    })
                                    @Html.ValidationMessageFor(a => a.TipoDeSangre, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label for="exampleEmail11" class="">Teléfono</label>
                                    @* <input placeholder="Precio" class="form-control"> *@
                                    @Html.TextBoxFor(a => a.Telefono, new
                                    {
                                    type = "number",
                                    @class = "form-control",
                                    @placeholder = "Telefono"
                                    })
                                    @* @Html.EditorFor(a => a.Producto.Precio, new { htmlAttributes = new { @type = "number",
                                        @class="form-control", @placeholder="Precio"}}) *@
                                    @Html.ValidationMessageFor(a => a.Telefono, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label for="exampleEmail11" class="">Celular</label>
                                    @* <input placeholder="Precio" class="form-control"> *@
                                    @Html.TextBoxFor(a => a.Celular, new
                                    {
                                    type = "number",
                                    @class = "form-control",
                                    @placeholder = "Celular"
                                    })
                                    @* @Html.EditorFor(a => a.Producto.Precio, new { htmlAttributes = new { @type = "number",
                                        @class="form-control", @placeholder="Precio"}}) *@
                                    @Html.ValidationMessageFor(a => a.Celular, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label for="exampleEmail11" class="">Email</label>
                                    @* <input placeholder="Precio" class="form-control"> *@
                                    @Html.TextBoxFor(a => a.Email, new
                                    {
                                    @class = "form-control",
                                    @placeholder = "Email"
                                    })
                                    @* @Html.EditorFor(a => a.Producto.Precio, new { htmlAttributes = new { @type = "number",
                                        @class="form-control", @placeholder="Precio"}}) *@
                                    @Html.ValidationMessageFor(a => a.Email, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label for="exampleEmail11" class="">Contraseña</label>
                                    @Html.TextBoxFor(a => a.Contrasennia, new
                                    {
                                    @type="password",
                                    @class = "form-control",
                                    @placeholder = "Contraseña"
                                    })
                                    @Html.ValidationMessageFor(a => a.Contrasennia, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">NIT</label>
                                    @* <input placeholder="Descripción Producto" class="form-control"> *@
                                    @Html.TextBoxFor(a => a.Nit, new
                                    {
                                    type = "number",
                                    @class = "form-control",
                                    @placeholder = "NIT",
                                    @required = "required",
                                    })
                                    @Html.ValidationMessageFor(a => a.Nit, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label for="exampleEmail11" class="">DPI</label>
                                    @* <input placeholder="Precio" class="form-control"> *@
                                    @Html.TextBoxFor(a => a.Dpi, new
                                    {
                                    @class = "form-control",
                                    @placeholder = "DPI"
                                    })
                                    @* @Html.EditorFor(a => a.Producto.Precio, new { htmlAttributes = new { @type = "number",
                                        @class="form-control", @placeholder="Precio"}}) *@
                                    @Html.ValidationMessageFor(a => a.Dpi, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Dirección</label>
                                    @Html.TextBoxFor(a => a.Direccion, new
                                    {
                                    @class = "form-control",
                                    @placeholder = "Direccion"
                                    })
                                    @Html.ValidationMessageFor(a => a.Direccion, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Número de afiliación</label>
                                    @Html.TextBoxFor(a => a.no_IGGS, new
                                    {
                                    @class = "form-control",
                                    @placeholder = "Número de Afiliación"
                                    })
                                    @Html.ValidationMessageFor(a => a.no_IGGS, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">¿Está afiliado?</label>
                                    <select asp-for="esta_Afiliado" class="form-control">
                                        <option value="false">No</option>
                                        <option value="true">Sí</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Seguro EPSS</label>
                                    <select class="form-control select2bs4" style="width: 100%;" asp-for="@Model.SeguroEpssId"
                                            asp-items="@Model.SeguroEpssSelectListItems">
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label>Código EPSS</label>
                                    @Html.TextBoxFor(a => a.CodigoEPS, new
                                    {
                                    @class = "form-control",
                                    @placeholder = "Codigo EPSS"
                                    })
                                    @Html.ValidationMessageFor(a => a.CodigoEPS, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Religion</label>
                                    @Html.TextBoxFor(a => a.Religion, new
                                    {
                                    @class = "form-control",
                                    @placeholder = "Religion"
                                    })
                                    @Html.ValidationMessageFor(a => a.Religion, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Ocupacion</label>
                                    @Html.TextBoxFor(a => a.Ocupacion, new
                                    {
                                    @class = "form-control",
                                    @placeholder = "Ocupacion"
                                    })
                                    @Html.ValidationMessageFor(a => a.Ocupacion, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Estado Civil</label>
                                    @Html.TextBoxFor(a => a.EstadoCivil, new
                                    {
                                    @class = "form-control",
                                    @placeholder = "Estado Civil"
                                    })
                                    @Html.ValidationMessageFor(a => a.EstadoCivil, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Contacto Emergencia</label>
                                    @Html.TextBoxFor(a => a.ContactoEmergencia, new
                                    {
                                    @class = "form-control",
                                    @placeholder = "Contacto Emergencia"
                                    })
                                    @Html.ValidationMessageFor(a => a.ContactoEmergencia, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Número Contacto Emergencia</label>
                                    @Html.TextBoxFor(a => a.NumeroContactoEmergencia, new
                                    {
                                    @class = "form-control",
                                    @placeholder = "Numero Contacto Emergencia"
                                    })
                                    @Html.ValidationMessageFor(a => a.NumeroContactoEmergencia, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Nombre del contacto de emergencia</label>
                                    @Html.TextBoxFor(a => a.NombreContactoEmergencia, new
                                    {
                                    @class = "form-control",
                                    @placeholder = "Nombre Contacto Emergencia"
                                    })
                                    @Html.ValidationMessageFor(a => a.NombreContactoEmergencia, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Nacionalidad</label>
                                    @Html.TextBoxFor(a => a.Nacionalidad, new
                                    {
                                    @class = "form-control",
                                    @placeholder = "Nacionalidad"
                                    })
                                    @Html.ValidationMessageFor(a => a.Nacionalidad, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">País de residencia</label>
                                    @Html.TextBoxFor(a => a.PaisResidencia, new
                                    {
                                    @class = "form-control",
                                    @placeholder = "País de residencia"
                                    })
                                    @Html.ValidationMessageFor(a => a.PaisResidencia, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Departamento de residencia</label>
                                    @Html.TextBoxFor(a => a.DepartamentoResidencia, new
                                    {
                                    @class = "form-control",
                                    @placeholder = "Departamento de residencia"
                                    })
                                    @Html.ValidationMessageFor(a => a.DepartamentoResidencia, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="position-relative form-group">
                                    <label class="">Municipio de residencia</label>
                                    @Html.TextBoxFor(a => a.MunicipioResidencia, new
                                    {
                                    @class = "form-control",
                                    @placeholder = "Municipio de residencia"
                                    })
                                    @Html.ValidationMessageFor(a => a.MunicipioResidencia, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-md-4">
                                <label>Tipo de paciente</label>
                                <select class="form-control" asp-for="@Model.TipoPacienteId"
                                        asp-items="@Model.TipoPacienteSelectListItems">
                                </select>
                            </div>
                            <div class="col-md-4">
                                <label>Modalidad de atención</label><br />
                                <select class="form-control select2bs4" style="width: 100%;" asp-for="@Model.ModalidadAtencion"
                                        asp-items="@Model.ModalidadAtencionSelectListItems">
                                </select>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="main-card mb-3 card col-md-12">
                    <div class="card-body">
                        <div class="form-row">
                            <div class="col-md-12">
                                <h5><b>Beneficiarios EPSS</b></h5>
                                <hr>
                            </div>
                            <div class="col-md-3">
                                <div class="position-relative form-group">
                                    <label class="">Tipo</label>
                                    <select data-bind="value: tipoNuevoBeneficiario" class="form-control">
                                        <option id="Titular">Titular</option>
                                        <option id="Beneficiario">Beneficiario</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="position-relative form-group">
                                    <label class="">Nit</label>
                                    <input data-bind="value: nitNuevoBeneficiario" class="form-control" placeholder="Nit" />
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="position-relative form-group">
                                    <label class="">Nombre</label>
                                    <input data-bind="value: nombreNuevoBeneficiario" class="form-control" placeholder="Nombre" />
                                </div>
                            </div>
                            <div class="col-md-3" style="text-align: center; margin: auto;">
                                <button class="btn btn-primary" style="color:white" data-bind="click: agregarBeneficiarioEpss">
                                    <i class="fas fa-add"></i> Agregar
                                </button>
                            </div>
                            <div class="col-md-12">
                                <br>
                                <div class="table-responsive">
                                    <table class="table">
                                        <thead>
                                            <tr>
                                                <th>Tipo</th>
                                                <th>Nit</th>
                                                <th>Nombres</th>
                                                <th></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <!--ko foreach: beneficiariosEpss-->
                                            <tr>
                                                <td data-bind="text: Tipo"></td>
                                                <td data-bind="text: Nit"></td>
                                                <td data-bind="text: Name"></td>
                                                <td><button class="btn btn-danger" data-bind="click:$root.quitarBeneficiarioEpss">Quitar</button></td>
                                            </tr>
                                            <!--/ko-->
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="main-card mb-3 card col-md-12">
                    <div class="card-body">
                        <div class="form-row">
                            <div class="col-md-12">
                                <h5><b>Datos de IGSS</b></h5>
                                <hr>
                            </div>
                            <div class="col-md-3">
                                <label>Tipo de afiliación</label>
                                <select class="form-control" id="IgssTipoAfiliacion">
                                    <option value="Jubilado">Jubilado</option>
                                    <option value="Activo">Activo</option>
                                    <option value="Dependiente">Dependiente</option>
                                </select>
                            </div>
                            <div class="col-md-3">
                                <label>Número de afiliación</label>
                                @Html.TextBoxFor(p=>p.IgssNumeroAfiliacion, new {@class="form-control", @placeholder="Número de afiliación"})
                            </div>
                            <div class="col-md-3">
                                <label>Cantidad de dependientes</label>
                                @Html.TextBoxFor(p=>p.IgssCantidadDependientes, new {@class="form-control", @type="number"})
                            </div>
                            <div class="col-md-3">
                                <label>Parentesco de dependientes</label>
                                @Html.TextBoxFor(p=>p.IgssParentescoDependientes, new {@class="form-control", @placeholder="Parentesco de dependientes"})
                            </div>
                        </div>
                    </div>
                </div>
                <div class="main-card mb-3 card col-md-12">
                    <div class="card-body">
                        <div class="form-row">
                            <div class="col-md-12">
                                <h5><b>Información de nacimiento</b></h5>
                                <hr>
                            </div>
                            <div class="col-md-3">
                                <label>PAN (Peso al nacer)</label>
                                @Html.TextBoxFor(a => a.PesoAlNacer, new { @class = "form-control", @placeholder = "Peso al nacer" })
                                @Html.ValidationMessageFor(a => a.PesoAlNacer, "", new { @class = "text-danger" })
                            </div>
                            <div class="col-md-3">
                                <label>Talla (estatura del niño)</label>
                                @Html.TextBoxFor(a => a.Talla, new { @class = "form-control", @placeholder = "Talla" })
                                @Html.ValidationMessageFor(a => a.Talla, "", new { @class = "text-danger" })
                            </div>
                            <div class="col-md-3">
                                <label>Circunferencia cefálica (CC)</label>
                                @Html.TextBoxFor(a => a.CircunferenciaCefalica, new { @class = "form-control", @placeholder = "Circunferencia cefálica" })
                                @Html.ValidationMessageFor(a => a.CircunferenciaCefalica, "", new { @class = "text-danger" })
                            </div>
                            <div class="col-md-3">
                                <label>Tipo de parto</label>
                                <select class="form-control" asp-for="TipoParto">
                                    <option value="normal">Normal</option>
                                    <option value="cesarea">Cesarea</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="main-card mb-3 card col-md-12">
                    <div class="card-body">
                        <div class="form-row">
                            <div class="col-md-12">
                                <h5><b>Fotografía del paciente</b></h5>
                                <hr>
                            </div>
                            <div class="col-md-12">
                                Adjunte una fotografía del paciente al inicio del tratamiento<br>
                                <input type="file" class="form-control" id="fotografia-paciente" />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="main-card mb-3 card col-md-12">
                    <div class="card-body">
                        <div class="form-row">
                            <div class="col-md-12">
                                <h5><b>Archivos</b></h5>
                                <button class="btn btn-primary" data-bind="click:agregarArchivo">Agregar</button>
                                <hr>
                            </div>
                            <div class="col-md-12">
                                <table class="table table-hover">
                                    <thead>
                                        <tr>
                                            <th>Nombre de archivo</th>
                                            <th>Subir archivo</th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <!--ko foreach: archivos-->
                                        <tr>
                                            <td><input class="form-control" data-bind="value:Nombre" placeholder="Nombre" /></td>
                                            <td><input class="form-control" type="file" data-bind="attr:{id:IdInputFile}" /></td>
                                            <td><button class="btn btn-danger" data-bind="click:$root.quitarArchivo">Quitar</button></td>
                                        </tr>
                                        <!--/ko-->
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="main-card mb-3 card col-md-12">
                    <div class="card-body">
                        <div class="form-row">
                            <div class="col-md-12">
                                <h5><b>Firma</b></h5>
                                <hr>
                            </div>
                            <div class="col-md-12">
                                <canvas id="firma-registro" style="width: 900px;height: 300px;background-color: #ffffff;border: 1px solid black;"></canvas>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="tab-pane" id="tab-eg2-1" role="tabpanel">
                <div class="main-card mb-3 card">
                    <div class="card-body">
                        <div class="form-row">
                            <div class="col-md-12">
                                <h5><b>Antecedentes familiares</b></h5>
                                <hr>
                            </div>
                            <div class="col-md-12">
                                <div class="table-responsive">
                                    <table id="tabla-antecedentes-familiares">
                                        <thead>
                                            <tr>
